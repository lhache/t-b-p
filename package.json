{
  "version": "0.0.12",
  "name": "tbp-app",
  "description": "tbp - frontend app logic",
  "homepage": "https://github.com/lhache/tbp-cms#readme",
  "main": "static/js/main.js",
  "bugs": {
    "url": "https://github.com/lhache/tbp-cms/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lhache/t-b-p.git"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "counterpart": "^0.18.1",
    "flexbox-react": "^4.4.0",
    "history": "^4.6.3",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "mobile-detect": "^1.3.6",
    "react": "^16.0.0",
    "react-cookie-banner": "0.0.18",
    "react-dom": "^16.0.0",
    "react-image-gallery": "^0.8.3",
    "react-interpolate-component": "^0.11.0",
    "react-rater": "^0.4.3",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-select": "git://github.com/JedWatson/react-select.git#master",
    "react-svg": "^2.1.21",
    "react-translate-component": "^0.14.0",
    "react-truncate": "^2.1.5",
    "react-virtualized-select": "^3.1.0",
    "redux": "^3.7.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer": "7.1.0",
    "babel-cli": "^6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "^3.0.0",
    "babel-runtime": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "css-loader": "0.28.1",
    "dotenv": "4.0.0",
    "eslint": "3.19.0",
    "eslint-config-react-app": "^1.0.4",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.33.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.0.1",
    "expect": "^1.20.2",
    "extract-text-webpack-plugin": "2.1.0",
    "fetch-mock": "^5.12.0",
    "file-loader": "0.11.1",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.28.0",
    "jest": "20.0.3",
    "jest-cli": "^20.0.4",
    "netlify-cli": "^1.2.2",
    "node-sass": "^4.5.3",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.0.2",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.5",
    "promise": "7.1.1",
    "react-dev-utils": "^3.0.0",
    "react-error-overlay": "^1.0.7",
    "redux-mock-store": "^1.2.3",
    "regenerator-runtime": "^0.10.5",
    "style-loader": "0.17.0",
    "sw-precache-webpack-plugin": "0.9.1",
    "url-loader": "0.5.8",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.4.5",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "start-js": "node scripts/start.js",
    "build": "npm run build-css && node scripts/build.js",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "test": "node scripts/test.js --env=jsdom",
    "test:watch": "npm test -- --watch",
    "deploy-surge": "surge --project ./build --domain t-b-p.surge.sh",
    "deploy-netlify": "netlify deploy",
    "prepub": "npm run build && cp ./package.json build"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
